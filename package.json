{
  "name": "rpg-img-gen",
  "version": "1.0.0",
  "description": "RPG Template Generator and OBS Overlay System for AI Image Generation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "build": "npm run frontend:build",
    "setup": "npm install && cd frontend && npm install",
    "setup:prod": "npm ci && cd frontend && npm ci && npm run build",
    "db:init": "node scripts/init-db.js",
    "railway:setup": "npm install && cd frontend && npm install && npm run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "rpg",
    "image-generation",
    "obs",
    "twitch",
    "template",
    "ai"
  ],
  "author": "Kevin Huff",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-sqlite3": "^0.9.13",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
